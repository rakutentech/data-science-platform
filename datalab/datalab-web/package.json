{
  "name": "datalab-web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build:dev-front": "webpack --config webpack/webpack.dev.js",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "build:prod": "webpack --config webpack/webpack.prod.js",
    "fake-client": "webpack-dev-server --hot --open --env.FAKE_BACKEND=true --config webpack/webpack.dev.js",
    "client": "webpack-dev-server --hot --open --config webpack/webpack.dev.js",
    "server": "nodemon src/server.js  --exec babel-node",
    "start:dev-front": "yarn run fake-client",
    "start:dev": "concurrently \"yarn run client\" \"NODE_ENV=development yarn run server\"",
    "start:prod": "NODE_ENV=production yarn run server",
    "test": "jest --updateSnapshot",
    "swagger": "concurrently \"swagger-jsdoc -d swaggerDef.js -o src/swagger.json\" \"swagger-jsdoc -d adminSwaggerDef.js -o src/adminSwagger.json\"",
    "dashboard": "webpack-dashboard -t 'datalab' -- webpack-dev-server --hot --open --config webpack/webpack.dev.js",
    "test:coverage": "nyc npm run test",
    "lint:js": "eslint ./src ./webpack -f table --ext .js --ext .jsx",
    "lint:scss": "stylelint ./src/**/*.scss --syntax scss",
    "lint": "npm run lint:js && npm run lint:scss",
    "flow": "flow"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.3.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "gulp-sass": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-transform-stub": "^1.0.0",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "react-bootstrap": "1.0.0-beta.5",
    "react-test-renderer": "^16.7.0",
    "redux-devtools": "^3.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "brace": "^0.11.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "flow-bin": "^0.90.0",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.4.0",
    "log4js": "^4.0.2",
    "mdbreact": "^4.11.0",
    "node-sass": "^4.12.0",
    "react": "^16.7.0",
    "react-ace": "^7.0.1",
    "react-bootstrap-table": "^4.3.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.7.0",
    "react-dropzone-component": "^3.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "sinon": "^7.3.1",
    "stylelint": "^9.9.0",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-dist": "^3.20.5",
    "swagger-ui-express": "^4.0.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
